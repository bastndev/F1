    "configuration": {
      "title": "F1 Extension",
      "properties": {
        "f1.shortcuts": {
          "type": "object",
          "default": {},
          "description": "Custom shortcuts defined by the user. Each key is the shortcut combination (e.g., 'ctrl+f1') and the value is the command to execute. Only F1-F12 combinations are supported: ctrl+f1-f12, alt+f1-f12, shift+f1-f12.",
          "patternProperties": {
            "^(ctrl|alt|shift)\\+f([1-9]|1[0-2])$": {
              "type": "string",
              "description": "Command to execute when this F1-F12 key combination is pressed"
            }
          },
          "additionalProperties": false
        },
        "f1.shortcuts.autoRegister": {
          "type": "boolean",
          "default": true,
          "description": "Automatically register user-defined shortcuts as keybindings"
        }
      }
    }
