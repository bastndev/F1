    "configuration": {
      "title": "F1 Extension",
      "properties": {
        "f1.shortcuts": {
          "type": "object",
          "default": {},
          "description": "Custom shortcuts defined by the user. Each key is the shortcut combination (e.g., 'ctrl+f2') and the value is the command to execute. Only F2-F12 combinations are supported: ctrl+f2-f12, shift+f2-f12, ctrl+shift+f2-f12. F1 is reserved for built-in functionality.",
          "patternProperties": {
            "^(ctrl|shift|ctrl\\+shift)\\+f([2-9]|1[0-2])$": {
              "type": "string",
              "description": "Command to execute when this F2-F12 key combination is pressed"
            }
          },
          "additionalProperties": false
        },
        "f1.shortcuts.autoRegister": {
          "type": "boolean",
          "default": true,
          "description": "Automatically register user-defined shortcuts as keybindings"
        }
      }
    }
  },
  
  "scripts": {
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "node ./out/test/runTest.js"
  },
